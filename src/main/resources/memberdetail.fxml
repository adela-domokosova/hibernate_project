<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.controller.MemberDetailController"
            prefHeight="400.0" prefWidth="600.0">
    <BorderPane xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml">

    <top>
        <Label text="Members detail" style="-fx-font-size: 20px; -fx-padding: 10;" />
        <Button text="Back" onAction="#switchToHome" />
    </top>
        <center>
            <VBox spacing="10" alignment="CENTER_LEFT" style="-fx-padding: 20;">
                <Label fx:id="nameLabel" text="Name: " />
                <Label fx:id="registrationLabel" text="Registration date: " />
                <Label fx:id="emailLabel" text="Email: " />
            </VBox>
        </center>
        <bottom>
            <HBox spacing="10" style="-fx-padding: 10;">

            </HBox>
        </bottom>
</BorderPane>
        </AnchorPane>

