<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.controller.MemberDetailController"
            prefHeight="400.0" prefWidth="600.0">
    <BorderPane xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml">

    <top>
        <Label text="Members detail" style="-fx-font-size: 20px; -fx-padding: 10;" />
        <Button text="Back" onAction="#switchToHome" />
    </top>
    <center>
        <VBox spacing="10" style="-fx-padding: 10;">
            <ListView fx:id="memberListView" prefHeight="300.0" prefWidth="400.0" />
        </VBox>
    </center>
    <bottom>
        <HBox spacing="10" style="-fx-padding: 10;">
            <Button text="HOME" onAction="#switchToHome" />
        </HBox>
    </bottom>
</BorderPane>
        </AnchorPane>

